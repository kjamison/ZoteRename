<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	
	<Description about="urn:mozilla:install-manifest">
		<em:id>zoterename@kjamison.umn.edu</em:id>
		<em:name>ZoteRename</em:name>
		<em:version>1.3.4</em:version>
		<em:description>File Renaming Utility for Zotero.</em:description>
		<em:creator>Keith Jamison</em:creator>
		<em:homepageURL>http://umn.edu/home/kjamison/zoterename/</em:homepageURL>
		<em:optionsURL>chrome://zoterename/content/options.xul</em:optionsURL>
		<em:updateURL>https://netfiles.umn.edu/users/kjamison/ffext/update-zoterename.rdf</em:updateURL>
		<em:type>2</em:type> <!-- type=extension -->
		
		<!-- Requires Zotero -->
		<em:requires>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:name>Zotero</em:name>
				<em:homepageURL>http://www.zotero.org</em:homepageURL>
				<em:minVersion>1.0.0b4.r5</em:minVersion>
				<em:maxVersion>4.*</em:maxVersion>
			</Description>
		</em:requires>
		
		<!-- Requires Firefox -->
		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>2.0</em:minVersion>
				<em:maxVersion>1000.*</em:maxVersion>
			</Description>
		</em:targetApplication>
		
		<!-- Or Zotero Standalone -->
		<em:targetApplication>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:minVersion>3.0b1</em:minVersion>
				<em:maxVersion>4.0.*</em:maxVersion>
			</Description>
		</em:targetApplication>	
	</Description>	
</RDF>
